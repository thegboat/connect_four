<section class="board">
  <div id="result">You Lose</div>
  <%= link_to "Start Game", controller: "main", action: "index" %>
  <table>
    <tbody>
      <% 5.step(0,-1) do |y| %>
        <tr>
        <% 0.step(6) do |x| %>
            <td><button type="button" name="<%= x.to_s + '_' + y.to_s %>"></button></td>
        <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
</section>