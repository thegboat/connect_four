<section class="wrapper">
  <div id="result"></div>
  <button id="new_game" type="button">New Game</button>
  <table class="board">
    <tbody>
      <% 5.step(0,-1) do |y| %>
        <tr>
        <% 0.step(6) do |x| %>
            <td><button type="button" name="<%= x.to_s + '_' + y.to_s %>"></button></td>
        <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
</section>